  <!-- remove old bundle registration -->
  <records prefix="plone.bundles/bda-plone-shop-bundle" remove="true"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
  </records>


  <!-- register bundle without merge with -->
  <records prefix="plone.bundles/bda-plone-shop-bundle"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="depends">jquery</value>
    <value key="jscompilation">++resource++shop/shop-bundle.js</value>
    <value key="csscompilation">++resource++shop/shop-bundle.css</value>
    <value key="compile">False</value>
    <value key="load_async">True</value>
    <!-- Do not merge bundle because of webfont path -->
  </records>

